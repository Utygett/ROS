<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence>
            <Action ID="Subscriber" num_output="${a}"/>
            <Action ID="Publisher" num_of_print="${a}"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="Publisher">
            <input_port name="num_of_print"/>
        </Action>
        <Action ID="Subscriber">
            <output_port name="num_output"/>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

